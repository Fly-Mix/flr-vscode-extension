{
  "publisher": "LincolnLaw",
  "name": "flr",
  "displayName": "Flr",
  "description": "A Flutter Resource Manager VSCode Extension",
  "icon": "resources/flr-icon.png",
  "version": "3.3.3",
  "repository": "https://github.com/Fly-Mix/flr-vscode-extension",
  "license": "MIT",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:pubspec.yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flr.init",
        "title": "Add Flr Config",
        "icon": {
          "light": "resources/light/add.svg",
          "dark": "resources/dark/add.svg"
        }
      },
      {
        "command": "flr.openFile",
        "title": "Open File"
      },
      {
        "command": "flr.stopMonitor",
        "title": "Stop Monitor"
      },
      {
        "command": "flr.startMonitor",
        "title": "Start Monitor"
      },
      {
        "command": "flr.regenerated",
        "title": "Re Generate r.g.dart",
        "icon": {
          "light": "resources/light/refresh.png",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "flr.init",
          "when": "view == flr",
          "group": "navigation"
        },
        {
          "command": "flr.regenerated",
          "when": "view == flr",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "flr.stopMonitor",
          "when": "view == flr && viewItem == file && isMonitorEnabled",
          "group": "inline"
        },
        {
          "command": "flr.startMonitor",
          "when": "view == flr && viewItem == file && !isMonitorEnabled",
          "group": "inline"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "icon": "resources/flr-icon.png",
          "id": "flr",
          "name": "Flr(Assets Manager)",
          "when": "isPubspecYamlExist"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/md5": "^2.1.33",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/rimraf": "^2.0.2",
    "@types/vscode": "^1.41.0",
    "@types/yaml": "^1.9.6",
    "glob": "^7.1.5",
    "tslint": "^5.20.0",
    "typescript": "^3.8.0",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "glob": "^7.1.5",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.0.2",
    "yaml": "^2.8.1"
  }
}
